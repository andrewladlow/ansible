directory = /home/{{ username }}/downloads
session = /home/{{ username }}/.session
schedule = watch_directory,5,5,load_start=/home/{{ username }}/~watch/*.torrent
schedule = low_diskspace,5,60,close_low_diskspace=10240M
port_range = 55950-56000
port_random = yes
check_hash = yes
trackers.use_udp.set = yes
encryption = allow_incoming,enable_retry,prefer_plaintext
dht = disable
protocol.pex.set = no
scgi_port = 127.0.0.1:5000
